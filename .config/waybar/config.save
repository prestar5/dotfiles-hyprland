{
  "layer": "top",
    "modules-left": ["custom/arch", "clock", "tray"],
    "modules-center": ["hyprland/workspaces"],
    "modules-right": ["group/systemstats", "backlight", "pulseaudio", "network", "battery"],
    "custom/arch": {
      "format": "",
      "tooltip-format": "i use arch btw",
      "on-click": "exec .config/rofi/launchers/type-6/launcher.shq"
    },
    "hyprland/workspaces": {
      "format": "{name}",
      "format": "{icon}",
      "tooltip": false,
      "format-icons": {
	"1": "",
	"2": "",
	"3": "",
	"4": "",
	"5": "󰆈",
	"6": "",
	"7": "󰙯",
      },
    },
    "clock": {
      "format": "<span color='#b4befe'> </span> {:%H:%M}",
      "tooltip-format": "{:%m-%d-%Y}",
    },
    "backlight": {
      "device": "intel_backlight",
      "tooltip": false,
      "format": "<span color='#b4befe'>{icon}</span> {percent}%",
      "format-icons": ["", "", "", "", "", "", "", "", ""]
    },
    "pulseaudio": {
      "format": "<span color='#b4befe'>{icon}</span> {volume}%",
      "format-muted": "",
      "tooltip": false,
      "format-icons": {
        "headphone": "",
        "default": ["", "", "󰕾", "󰕾", "󰕾", "", "", ""]
      },
      "scroll-step": 1,
    },
    "network": {
      "interface": "wlp5s0",
      "format": "{ifname}",
      "format-wifi": "<span color='#b4befe'> </span> {essid}",
      "format-ethernet": "{ipaddr}/{cidr} ",
      "format-disconnected": "<span color='#b4befe'>󰖪 </span>Disconnected",
      "tooltip": false,
    },
    "battery": {
      "format": "<span color='#b4befe'>{icon}</span> {capacity}%",
      "format-icons": ["", "", "", "", "", "", "", "", "", ""],
      "format-charging": "<span color='#b4befe'></span> {capacity}%",
      "tooltip": false,
    },
    "tray": {
      "icon-size": 14,
      "spacing": 5
    },
    "cpu": {
      "format": "<span color='#b4befe'> </span> {usage}%"
    },
    "memory": {
      "format": "<span color='#b4befe'> </span> {percentage}%"
    },
    "group/systemstats": {
       "orientation": "horizontal",
       "modules": [
		"cpu",
		"memory",
	]
     },
}
